<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping
>
    <class
        name="edu.se.evolution.kenyon.scm.CommitData"
    >

        <id
            name="id"
            column="id"
            type="java.lang.Long"
        >
            <generator class="native">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-CommitData.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="filename"
            type="java.lang.String"
            update="true"
            insert="true"
            column="filename"
        />

        <property
            name="newRevEndLine"
            type="int"
            update="true"
            insert="true"
            column="newRevEndLine"
        />

        <property
            name="newRevStartLine"
            type="int"
            update="true"
            insert="true"
            column="newRevStartLine"
        />

        <property
            name="oldRevEndLine"
            type="int"
            update="true"
            insert="true"
            column="oldRevEndLine"
        />

        <property
            name="oldRevStartLine"
            type="int"
            update="true"
            insert="true"
            column="oldRevStartLine"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-CommitData.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
